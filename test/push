## vim:ft=zsh
somefile=$RANDOM
touch $somefile
git add $somefile
git commit --quiet -m $somefile
pkg push
if [[ $repo == *-bare ]] {
	[[ $(git --git-dir=$test_proot/$repo log --pretty=oneline | head -1) == *$somefile ]]
} else {
	[[ $(git --git-dir=$test_proot/$repo/.git log --pretty=oneline | head -1) == *$somefile ]]
}
